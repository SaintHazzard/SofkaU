SubProceso barraseparacion
	Escribir "   	|-----------------------------------------------------------------------------------------------------------------------------------------------------------------------|";
FinSubProceso


SubProceso columxfilas
	
	Definir matriz, i, k, a, cont, matriz2, matriz3, fila, columna, indice, v Como Entero;
	definir filas Como Caracter;
	Dimension matriz(10,10);
	Dimension matriz2(10,10);
	Dimension matriz3(10,10);
	Dimension filas(5);
	filas[0]<-"F";
	filas[1]<-"I";
	filas[2]<-"L";
	filas[3]<-"A";
	filas[4]<-"S";
	a<-0;
	cont<-0;
	v<-0;
	
	para i<-0 Hasta 9 Con Paso 1 Hacer
		para k<-0 Hasta 9 Con Paso 1 Hacer
			matriz(i,k)<-k+1;
			
			
			
			
			
		FinPara
		
		
		
		
	FinPara
	
	
	para i<-0 Hasta 9 Con Paso 1 Hacer
		para k<-0 Hasta 9 Con Paso 1 Hacer
			matriz2(i,k)<-a+1;
			
			
			
		FinPara
		a<-a+1;
		
		
		
	FinPara
	cabeza;	
	
	Escribir Sin Saltar "   |       ";
	para indice <-0 hasta 9 Con Paso 1 Hacer
		
		Escribir Sin Saltar "|", "",     "   ", indice,"           ";
		
		
		
		
		
	FinPara
	Escribir Sin Saltar "|";
	Escribir "";
	barraseparacion;
	
	para i<-0 Hasta 2	 Con Paso 1 Hacer
		para k<-0 Hasta 9 Con Paso 1 Hacer
			
			
			Si matriz2(i,k)>=0 y matriz2(i,k)<4 y matriz(i,k)=1 Entonces
				
				
				Escribir Sin Saltar  "   |   ",i,"    | ",  matriz(i,k),"x", matriz2(i,k), "";
			SiNo
				Escribir Sin Saltar "   |       | ", matriz(i,k),"x", matriz2(i,k); 
				
				
			FinSi
			
			
			
		FinPara
		Escribir "";
		barraseparacion;
		
	FinPara
	para i<-3 Hasta 7	 Con Paso 1 Hacer
		para k<-0 Hasta 9 Con Paso 1 Hacer
			
			
			Si matriz2(i,k)>3 y matriz2(i,k)<=8  y matriz(i,k)<=1 Entonces
				
				
				Escribir Sin Saltar filas(v), " 	 |   ",i,"   |  ",  matriz(i,k),"x", matriz2(i,k), "";
				v<-v+1;
			SiNo
				Escribir Sin Saltar "   |       | ", matriz(i,k),"x", matriz2(i,k); 
				
				
			FinSi
			
			
			
			
			
		FinPara
		Escribir "";
		barraseparacion;
		
	FinPara
	para i<-8 Hasta 9	 Con Paso 1 Hacer
		para k<-0 Hasta 9 Con Paso 1 Hacer
			
			
			Si matriz2(i,k)>7 y matriz2(i,k)<=10  y matriz(i,k)<=1 Entonces
				
				
				Escribir Sin Saltar " 	  |   ",i,"   |  ",  matriz(i,k),"x", matriz2(i,k), "";
			SiNo
				Si matriz2(i,k)=9 Entonces
					Escribir Sin Saltar "   |       | ", matriz(i,k),"x", matriz2(i,k);
				sino
					Escribir Sin Saltar "   |      | ", matriz(i,k),"x", matriz2(i,k); 
				FinSi
				
				
				
			FinSi
			
			
			
		FinPara
		Escribir "";
		barraseparacion;
		
	FinPara
	
	para i<-0 Hasta 9 Con Paso 1 Hacer
		para k<-0 Hasta 9 Con Paso 1 Hacer
			
			matriz3(i,k)<-matriz[i,k]*matriz2[i,k];
			
			
			
			
			
		FinPara
		
		
		
		
	FinPara
	Escribir "";
	Escribir "Indique fila";
	leer fila;
	Escribir "Indique columna";
	leer columna;
	Escribir matriz3(fila,columna);
FinSubProceso


	
	

