Proceso sin_titulo
	medflechai;
FinProceso


SubProceso medflechai
	definir i, asterisco, caract Como Entero;
	i<--0;
	asterisco<-0;
	caract<-0;
	Mientras i<=8 Hacer
		i<-i+1;
		
		Mientras asterisco<=8-i Hacer
			asterisco<-asterisco+1;
			Escribir Sin Saltar " ";
		FinMientras
		asterisco<-0;
		Mientras caract<0+i Hacer
			caract<-caract+1;
			Escribir Sin Saltar "*";
		FinMientras
		caract<-0;
		
		Escribir "";
		
		
	FinMientras

	
FinSubProceso



